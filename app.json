{
  "expo": {
    "name": "CryptoTracker",
    "slug": "CryptoTracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.million.cryptotracker",  
      "buildNumber": "1.0.0",
      "icon": "./assets/icon.png",  
      "splash": {
        "image": "./assets/splash-icon.png"
      },
      "supportsTablet": true,
      "infoPlist": { }
    },
    "android": {
      "bundleIdentifier": "com.million.cryptotracker",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [ ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
